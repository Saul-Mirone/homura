    "react-refresh": "^0.9.0",
    "react-test-renderer": "^16.12.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "latest",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.0.3",
