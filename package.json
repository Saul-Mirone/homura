{
  "main": ".webpack/main",
  "name": "homura",
  "productName": "Homura",
  "version": "0.1.0",
  "description": "Homura RSS Reader",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,pcss}": [
      "stylelint --ignore-path .eslintignore --fix",
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saul-Mirone/homura.git"
  },
  "author": {
    "name": "Saul-Mirone",
    "email": "Saul.Mirone@gmail.com",
    "url": "https://saul-mirone@github.io"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saul-mirone/homura/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux",
    "typescript",
    "ts",
    "postcss",
    "homura",
    "rss"
  ],
  "homepage": "https://github.com/Saul-Mirone/homura#readme",
